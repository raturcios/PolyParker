<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ricky.polyparker">


    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@drawable/parkingicon"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HybridActivity" android:screenOrientation="portrait"/>
        <activity android:name=".TabbedActivity" android:screenOrientation="portrait"/>
        <activity android:name=".ListTabActivity" android:screenOrientation="portrait"/>
        <activity android:name=".MapActivity" android:screenOrientation="portrait"/>
    </application>

</manifest>
